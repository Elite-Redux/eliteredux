script SootopolisCity_Gym_1F_EventScript_GiveScald{
	msgbox(format("Take this special Pokémon as a gift from me!"))
	givecustommon(SPECIES_KINGDRA, 70, ITEM_KINGDRANITE, ITEM_CHERISH_BALL, NATURE_MODEST, 0, 0, 0, 0, 252, 252, 4, 0, 0, 0, 0, 0, 0, MOVE_HYDRO_PUMP, MOVE_HURRICANE, MOVE_DRACO_METEOR, MOVE_FLASH_CANNON, FALSE)
	playfanfare(MUS_OBTAIN_ITEM)
	message(format("{PLAYER} got Juan's {COLOR BLUE}Kingdra{COLOR DARK_GRAY} as a gift!"))
	waitfanfare
	setflag(FLAG_RECEIVED_TM03)
	msgbox(SootopolisCity_Gym_1F_Text_JuanPostBattle, MSGBOX_DEFAULT)
	release
	end
}

script SootopolisCity_Gym_EventScript_CheckTrainers{
	//Check that you have defeated every trainer in that Gym and that you don't have that badge yet
	if(((!defeated(TRAINER_ANDREA)  || 
		 !defeated(TRAINER_CONNIE)  || 
		 !defeated(TRAINER_ANNIKA)  || 
		 !defeated(TRAINER_DAPHNE)  || 
		 !defeated(TRAINER_BRIDGET) ||
		 !defeated(TRAINER_TIFFANY) //||
	     //!defeated(TRAINER_CRISSY)  || 
		 //!defeated(TRAINER_BRIANNA) || 
	     //!defeated(TRAINER_OLIVIA)  || 
		 //!defeated(TRAINER_BETHANY) || 
		 ) &&
		 !flag(FLAG_BADGE08_GET))){
		msgbox(format("Juan: Sorry, but you need to defeat every trainer on this Gym before battling us."), MSGBOX_NPC)
		release
		end
	}
	else{
		//If you have beaten every trainer or you have the badge just return to the previous script
		return
	}
}